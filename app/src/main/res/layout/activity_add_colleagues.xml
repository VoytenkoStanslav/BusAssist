<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.app.admin.busassist.by.app.admin.busassist.ActivityAddAndReport.ActivityAddColleagues">

    <ImageButton
        android:id="@+id/imageBtnColl"
        android:layout_width="110dp"
        android:layout_height="140dp"
        android:layout_marginRight="126dp"
        android:layout_marginTop="16dp"
        android:scaleType="fitXY"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@android:drawable/ic_menu_add" />

    <TextView
        android:id="@+id/textView5p"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="183dp"
        android:text="Имя:"
        android:textColor="@color/cardview_dark_background"
        android:textSize="18sp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/editNameColl"
        android:layout_width="200dp"
        android:layout_height="40dp"
        android:layout_marginRight="16dp"
        android:layout_marginTop="174dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:maxLength="16"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView6p"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="333dp"
        android:text="Комментарий:"
        android:textColor="@color/cardview_dark_background"
        android:textSize="18sp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/editCommitColl"
        android:layout_width="200dp"
        android:layout_height="40dp"
        android:layout_marginRight="16dp"
        android:layout_marginTop="326dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:maxLength="16"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="232dp"
        android:text="Номер:"
        android:textColor="@color/cardview_dark_background"
        android:textSize="18sp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/editNumberColl"
        android:layout_width="200dp"
        android:layout_height="40dp"
        android:layout_marginRight="16dp"
        android:layout_marginTop="224dp"
        android:ems="10"
        android:inputType="number"
        android:maxLength="24"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/editSkypeColl"
        android:layout_width="200dp"
        android:layout_height="40dp"
        android:layout_marginRight="16dp"
        android:layout_marginTop="274dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:maxLength="16"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="282dp"
        android:text="Скайп:"
        android:textColor="@color/cardview_dark_background"
        android:textSize="18sp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/btnOkColl"
        android:layout_width="120dp"
        android:layout_height="50dp"
        android:text="Ок"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="24dp"
        app:layout_constraintTop_toBottomOf="@+id/editCommitColl" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Фото:"
        android:textColor="@color/cardview_dark_background"
        android:textSize="18sp"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="75dp"
        android:layout_marginLeft="16dp"
        app:layout_constraintLeft_toLeftOf="parent" />
</android.support.constraint.ConstraintLayout>
